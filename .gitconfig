[user]
	name = Warren Friedl
	email = wfriedl@wrightwoodtech.com
[github]
	user = wfriedl
    token = !security find-generic-password -gs \"GitHub API Token\" 2>&1 >/dev/null | awk '/password/ {print $2}' | tr -d \\\"

[diff]
[color]
    ui = auto
[alias]
    st = status
    ci = commit
    co = checkout
    di = diff
    dc = diff --cached
    amend = commit --amend
    aa = add --all
    head = !git l -1
    h = !git head
    r = !git --no-pager l -20
    ra = !git r --all
    ff = merge --ff-only
    pullff = pull --ff-only
    noff = merge --no-ff
    l = log --graph --abbrev-commit --date=relative 
    la = !git l --all
    fa = fetch --all
    pom = push origin master
    b = branch
	unstage = reset HEAD --

[format]
    pretty=format:%C(yellow)%h%Creset -%C(red)%d%Creset %s %Cgreen(%ar) %C(bold blue)<%an>%Creset

[merge]
    tool = vimdiff

